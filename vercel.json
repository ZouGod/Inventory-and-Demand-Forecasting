{
  "version": 2,
  "public": false,
  "functions": {
    "api/*.py": {
      "runtime": "python@3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "",
  "outputDirectory": "."
}